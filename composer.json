{
    "name":"bright-cloud-studio/gai-invoices",
    "description":"Connecting Contao to Google Sheets using the Google PHP API",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["gai", "invoices", "google", "sheets", "php", "api"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13",
        "google/apiclient": "v2.12.6",
        "phpmailer/phpmailer": "^6.8.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/gai_invoices/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/gai_invoices": "system/modules/gai_invoices"
            }
        }
    },
    "minimum-stability": "dev"
}
